/* huali/prot/prot.wxss */
.prot{
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.newmyimg{
  display: flex;
  width: 45%;
  margin: 3% 1% 0% 3%;
  border: 1px rgb(228, 227, 227) solid;
  justify-content: flex-start;  
  flex-wrap: wrap;
}

.newmyimg image{
  height: 200px;
  width: 100%;
  padding:10px;
}
.newmyimg>view:nth-of-type(1){
  width: 100%;
  height: 10px;
  margin:0px 0px 0px 10px; 
}
.newmyimg>view:nth-of-type(2){
  display: flex;
  justify-content:space-between;
  width: 100%;
  height: 20px;
  margin:40px 0px 10px 10px; 
}
/* 购物车图标 */
.gwc{
  width: 20px;
  height: 20px;
  text-align: center;
  line-height: 20px;
  border: 1px black solid;
  border-radius: 50%;
  margin-right: 10px;
}
.gwc image{
 width: 15px;
 height: 15px;
 margin-top: -7px;
 margin-left: -8px;
}
.jiesuan{
  width: 100%;
  height: 50px;
  line-height: 50px;
  color: white;
  font-size: 16px;
  text-align: center;
  background-color: black;
  position: fixed;
  bottom: 0px;
}
